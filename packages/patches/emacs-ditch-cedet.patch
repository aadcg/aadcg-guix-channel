From: =?UTF-8?q?Andr=C3=A9=20A=2E=20Gomes?= <andremegafone@gmail.com>
Date: Mon, 12 Sep 2022 20:11:03 +0300
Subject: [PATCH] Get rid of cedet.

---
 lisp/Makefile.in | 6 +-----
 1 file changed, 1 insertion(+), 5 deletions(-)

diff --git a/lisp/Makefile.in b/lisp/Makefile.in
index 80955abfb5b..25a755242ae 100644
--- a/lisp/Makefile.in
+++ b/lisp/Makefile.in
@@ -108,8 +108,7 @@ endif
 COMPILE_FIRST += $(lisp)/emacs-lisp/autoload.elc
 
 # Files to compile early in compile-main.  Works around bug#25556.
-MAIN_FIRST = ./emacs-lisp/eieio.el ./emacs-lisp/eieio-base.el \
-  ./cedet/semantic/db.el
+MAIN_FIRST = ./emacs-lisp/eieio.el ./emacs-lisp/eieio-base.el
 
 # Prevent any settings in the user environment causing problems.
 unexport EMACSDATA EMACSDOC EMACSPATH
@@ -385,9 +384,6 @@ gen-lisp: leim semantic
 leim: $(lisp)/international/titdic-cnv.elc
 	$(MAKE) -C ../leim all EMACS="$(EMACS)"
 
-semantic:
-	$(MAKE) -C ../admin/grammars all EMACS="$(EMACS:.%=../.%)"
-
 # Compile all Lisp files, but don't recompile those that are up to
 # date.  Some .el files don't get compiled because they set the
 # local variable no-byte-compile.
